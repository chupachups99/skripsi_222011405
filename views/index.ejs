<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dashboard Pariwisita KTIP BPS</title>
  <link href="../assets/styles.css" rel="stylesheet">

  <!-- <link href="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.2.1/flowbite.min.css" rel="stylesheet" /> -->
  <!-- <script src="https://cdn.tailwindcss.com"></script> -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
    rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.datatables.net/2.0.2/css/dataTables.dataTables.css" />
  <link rel="stylesheet" href="https://cdn.datatables.net/buttons/3.0.2/css/buttons.dataTables.css" />
  <link rel="stylesheet" href="https://cdn.datatables.net/fixedcolumns/5.0.0/css/fixedColumns.dataTables.css" />

  <style>
    body,
    div,
    p,
    span {
      font-family: 'Poppins';
    }

    a {
      text-decoration: underline;
    }
  </style>




</head>

<body>

  <%- include('header'); %>

    <div class="font-sans grid h-svh w-auto overflow-scroll gap-y-4 bg-slate-400">
      <!-- mulai wisatawan nusantara -->
      <div class="grid ml-3 mr-3" style="margin-top:4.6rem; border-radius: 20px;">
        <div class="font-semibold text-center bg-gray-700 text-white p-1"
          style="border-radius: 20px; font-size: 2.5rem; margin-bottom: 20px;">
          <h1 id="section1">Wisatawan Nusantara</h1>
        </div>
        <!-- wisnus overtime -->
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <!-- <div class="text-right mr-4" style="font-size: 14px;">Export: <a id="exportChart1" href="#">All</a> | <a
            href="#">Selected</a> </div> -->

          <div class="p-2 ml-1 mr-1 flex">
            <div class="w-1/2">
              <p class="text-xl text-black font-bold">Wisatawan Nusantara Waktu
                ke Waktu</p>
            </div>
            <div class="text-right w-1/2"><select id="provWisnus"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius:20px;">
                <!-- <option value="9999" disabled selected hidden>Pilih Provinsi atau Pulau</option> -->
                <option value="9999" selected>Indonesia</option>
                <option value="1">Pulau Sumatera</option>
                <option value="2">Pulau Jawa</option>
                <option value="3">Pulau Bali Nusra</option>
                <option value="4">Pulau Kalimantan</option>
                <option value="5">Pulau Sulawesi</option>
                <option value="6">Pulau Maluku</option>
                <option value="7">Pulau Papua</option>

              </select></div>

          </div>

          <div class="md:flex">

            <!-- <div class="text-right font-medium mr-8">Export: <a id="exportChart2" href="#">All</a> | <a
                href="#">Selected</a></div> -->
            <div id="barYearWisnus" class="md:w-2/5 h-96 pt-4 mr-1 shadow" style="border-radius: 20px;"></div>
            <!-- <div class="w-1/4 flex items-center justify-center ">
              <div 
                class="h-64 text-center text-2xl bg-sky-700 text-white rounded-xl font-semibold mr-3 shadow items-center justify-center p-4">
                <div id ="textWilayah" class="block h-1/2" style='font-size:1.5rem'>Total Perjalanan Wisatawan Nusantara</div>
                <div id="textIndicator" class="h-1/2 flex justify-center" style='font-size:3rem'> </div>
                
              </div>
            </div> -->


            <div id="lineYearWisnus" class="h-96 pt-4 md:w-3/5 ml-1 shadow" style="border-radius: 20px;"></div>
          </div>
        </div>

        <br>
        <!-- wisnus tujuan -->
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <!-- <div class="text-right mr-4" style="font-size: 14px;">Export: <a id="" href="#">All</a> | <a
            href="#">Selected</a> </div> -->
          <div class="text-left p-2 flex">
            <p class="font-bold w-2/3 text-xl">Wisatawan
              Nusantara Antarwilayah (Menurut Provinsi Tujuan)</p>
            <div class="text-right w-1/3">
              <select id="sAwalTujuan"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius: 20px;">
                <option disabled selected hidden>Pilih Rentang Waktu Awal</option>
              </select>
              <br>
              <select id="sAkhirTujuan"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius: 20px;">
                <option selected>Pilih Rentang Waktu Akhir</option>
              </select>
              <br>
              <button id="btnInterval" class="bg-sky-600 text-white rounded-lg px-2.5 mt-2" style="font-size: 12px;">
                Apply</button>
            </div>
          </div>


          <div class="md:grid md:grid-cols-2">
            <div id="cPlethTujuan" class="h-[39rem] mr-1 shadow" style="border-radius: 20px;"></div>
            <div id="donutTujuan" class="h-[39rem] ml-1 shadow" style="border-radius: 20px;"></div>
          </div>
        </div>


        <br>

        <!-- wisnus asal  -->
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <!-- <div class="text-right mr-4" style="font-size: 14px;">Export: <a id="" href="#">All</a> | <a
            href="#">Selected</a> </div> -->
          <div class="text-left p-2 flex">
            <p class="font-bold text-xl w-2/3">Wisatawan Nusantara Antarwilayah (Menurut Provinsi Asal)</p>
            <div class="text-right w-1/3">
              <select id="sAwalAsal"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius: 20px;">
                <option disabled selected hidden>Pilih Rentang Waktu Awal</option>
              </select>
              <br>
              <select id="sAkhirAsal"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius: 20px;">
                <option selected>Pilih Rentang Waktu Akhir</option>
              </select>
              <br>

              <button id="btnIntervalAsal" class="bg-sky-600 text-white rounded-xl px-2.5 mt-2"> Apply</button>
            </div>
          </div>

          <div class="lg:grid lg:grid-cols-2">
            <div id="cPlethAsal" class="h-[39rem] mr-1 shadow" style="border-radius: 20px;"></div>
            <div id="donutAsal" class="h-[39rem] ml-1 shadow" style="border-radius: 20px;"></div>
          </div>
        </div>
        <br>
        <!-- tabulasi  -->
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <!-- <div class="text-right ml-3 mr-3 "><button id="btnExportTab">Export Table</button></div> -->
          <div class="flex p-2">
            <div class="text-left font-bold text-xl w-2/3">Tabulasi Wisatawan Nusantara</div>
            <div class="text-right ml-3 mr-3 w-1/3">
              <select id="sTabJumlah"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm px-5 py-1 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius: 20px;">
                <option value="1">Bulanan</option>
                <option value="0">Tahunan</option>
                <option value="2">Dwiwulanan</option>
                <option value="3">Triwulanan</option>
                <option value="4">Caturwulanan</option>
                <option value="5">Semesteran</option>
              </select>
            </div>

          </div>
          <br>

          <div id="tabCont" class="h-96 ml-3 mr-3">
            <!-- <table id="tabulasiWisnus" class="display nowrap" width="100%"></table> -->
          </div>
        </div>

      </div>
      <!-- batas div wisatawan nusantara -->
      <!-- <br> -->

      <!-- mulai tpk  -->
      <div class="grid ml-3 mr-3 mt-2" style="border-radius: 20px;">
        <div class="font-semibold text-center bg-gray-700 text-white p-1"
          style="border-radius: 20px; font-size: 2.5rem; margin-bottom: 20px;">
          <h1 id="section2">Tingkat Penghunian Kamar</h1>
        </div>

        <!-- tpk overtime  -->

        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <div class="p-2 ml-1 mr-1 flex">
            <div class="w-2/3">
              <p class="text-xl text-black font-bold">TPK Waktu
                ke Waktu</p>
            </div>
            <div class="text-right w-1/3">
              <select id="provTPK"
                class="text-left text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm px-5 py-2.5 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300"
                style="border-radius:20px;">
                <option value="9999" disabled selected hidden>Pilih Provinsi</option>
                <option value="9999">Indonesia</option>
              </select>
            </div>
          </div>
          <div id="tpkCombine" class="h-96 ml-3 mr-3 mt-2">
          </div>


        </div>
        <br>
        <!-- tpk compare -->
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <div class="flex ml-3 mt-2">

            <div class="w-1/4 p-2">
              <div>
                <div>Pilih Tahun :</div>
                <div>
                  <input type="radio" value="0" id="singleTPK" name="seriesTPK">
                  <label>single series</label>
                  <br>
                  <div class="p-3">
                    <select id="minYear"
                      class=" disabled text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm p-2 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300">
                    </select>
                    to
                    <select id="maxYear"
                      class="disabled text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm p-2 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300">
                    </select>
                  </div>

                  <input type="radio" value="1" id="multiTPK" name="seriesTPK">
                  <label>multiple series</label>
                  <div id="yearBoxTPK" class="overflow-y-scroll h-16 p-3"></div>
                </div>
                <br>
                <div>Pilih Daerah (Maks. 5 wilayah) :</div>
                <div id="divWilayahTPK" class="overflow-y-scroll h-32">
                  <input type="checkbox" value="9999" class="checkBoxWilayahTPK" checked="true">
                  <label> Indonesia</label>
                  <br>
                </div>
              </div>

            </div>

            <div id="tpkBintang" class="h-96 w-3/4"></div>
          </div>
        </div>

      </div>
      <!-- batas tpk  -->
      <br>

      <div class="bg-white grid ml-3 mr-3 rounded shadow">
        <div class="font-semibold text-center text-2xl rounded-t-lg bg-gray-700 text-white p-4">
          <h1>Data Wisatawan</h1>
        </div>
        <div class="flex">
          <div id="mapStory" class="w-3/4 h-96"></div>
          <!-- <div class="w-1/4 h-96"></div> -->
        </div>
        <div class="text-center p-4">
          <button
            class="text-left text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm px-5 py-2.5 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300">&lt;
            Previous </button>

          <button id="nextBtnStory"
            class="text-left text-black bg-white border-2 border-neutral-400 font-medium rounded-lg text-sm px-5 py-2.5 inline-flex items-center outline-none focus:border-none focus:ring focus:ring-sky-300">Next
            &gt; </button>
        </div>
      </div>

      <div class="grid ml-3 mr-3 mt-2" style="border-radius: 20px;">
        <div class="font-semibold text-center bg-gray-700 text-white p-1"
          style="border-radius: 20px; font-size: 2.5rem; margin-bottom: 20px;">
          <h1 id="section3">Wisatawan Mancanegara</h1>
        </div>

        

        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <div class="p-2 ml-1 mr-1 flex">
            <div class="w-2/3">
              <p class="text-xl text-black font-bold">Kunjungan Wisatawan Mancanegara Waktu ke Waktu</p>
            </div>
          </div>
          <div class="md:flex">
            <div id="barWisman" class="md:w-2/5 h-[30rem]">bar chart</div>
            <div id="lineWisman" class="md:w-3/5 h-[30rem]">linechart</div>
            <!-- <div class="w-1/4 h-96"></div> -->
          </div>
        </div>
        <br>

        
        <div class="bg-slate-200 p-3 shadow" style="border-radius: 25px;">
          <div class="p-2 ml-1 mr-1 flex">
            <div class="w-2/3">
              <p class="text-xl text-black font-bold">Kunjungan Wisatawan Mancanegara Berdasarkan Kebangsaan dan<br>Rata-Rata Biaya </p>
            </div>
          </div>
          <div class="">
            <div id="geoWisman" class="h-[39rem]">mapchart</div>
            <div id="scatterWisman" class="h-[30rem]">scatter</div>
            <!-- <div class="w-1/4 h-96"></div> -->
          </div>
        </div>

      </div>

      <div class="bg-white grid ml-3 mr-3 rounded shadow">


      </div>

      <!-- footer -->
      <div class=""><%- include('footer'); %></div>
    </div>


  
    <script src="https://cdnjs.cloudflare.com/ajax/libs/echarts/5.5.0/echarts.min.js"
      integrity="sha512-k37wQcV4v2h6jgYf5IUz1MoSKPpDs630XGSmCaCCOXxy2awgAWKHGZWr9nMyGgk3IOxA1NxdkN8r1JHgkUtMoQ=="
      crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.16.2/xlsx.full.min.js"></script>
    <script type="text/javascript" src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"></script>
    <script src="https://cdn.datatables.net/2.0.7/js/dataTables.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/3.0.2/js/dataTables.buttons.js"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.2/js/buttons.html5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.10.1/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/fixedcolumns/5.0.0/js/dataTables.fixedColumns.js"></script>
    <script src="https://cdn.datatables.net/fixedcolumns/5.0.0/js/fixedColumns.dataTables.js"></script>
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.2.7/vfs_fonts.js"></script> -->
    <script type="module" src="../assets/script_index.js"></script>

</body>

</html>